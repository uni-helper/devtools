{
  "name": "@uni-helper/trpc-client",
  "version": "0.0.1",
  "description": "The tRPC uni-app client library",
  "author": "FliPPeDround <flippedround@qq.com>",
  "license": "MIT",
  "funding": "https://afdian.net/a/flippedround",
  "homepage": "https://github.com/flippedround/uni-devtools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flippedround/uni-devtools.git",
    "directory": "packages/trpc-client"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./links/httpLink": {
      "import": "./dist/links/httpLink.mjs",
      "require": "./dist/links/httpLink.cjs",
      "default": "./dist/links/httpLink.cjs"
    },
    "./links/httpBatchLink": {
      "import": "./dist/links/httpBatchLink.mjs",
      "require": "./dist/links/httpBatchLink.cjs",
      "default": "./dist/links/httpBatchLink.cjs"
    },
    "./links/splitLink": {
      "import": "./dist/links/splitLink.mjs",
      "require": "./dist/links/splitLink.cjs",
      "default": "./dist/links/splitLink.cjs"
    },
    "./links/loggerLink": {
      "import": "./dist/links/loggerLink.mjs",
      "require": "./dist/links/loggerLink.cjs",
      "default": "./dist/links/loggerLink.cjs"
    },
    "./links/wsLink": {
      "import": "./dist/links/wsLink.mjs",
      "require": "./dist/links/wsLink.cjs",
      "default": "./dist/links/wsLink.cjs"
    },
    "./shared": {
      "import": "./dist/shared/index.mjs",
      "require": "./dist/shared/index.cjs",
      "default": "./dist/shared/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "!**/*.test.*",
    "README.md",
    "dist",
    "links",
    "package.json",
    "shared",
    "src"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild"
  },
  "peerDependencies": {
    "@trpc/server": "10.45.2"
  },
  "devDependencies": {
    "@trpc/server": "10.45.2",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/node": "^20.10.0",
    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.0",
    "undici": "^6.0.1"
  }
}
