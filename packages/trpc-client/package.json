{
  "name": "@uni-helper/trpc-client",
  "version": "0.0.1",
  "description": "The tRPC uni-app client library",
  "author": "FliPPeDround <flippedround@qq.com>",
  "license": "MIT",
  "funding": "https://afdian.net/a/flippedround",
  "homepage": "https://github.com/flippedround/uni-devtools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flippedround/uni-devtools.git",
    "directory": "packages/trpc-client"
  },
  "bugs": {
    "url": "https://github.com/flippedround/uni-devtools/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./links/httpBatchLink": {
      "import": "./dist/links/httpBatchLink.mjs",
      "require": "./dist/links/httpBatchLink.js",
      "default": "./dist/links/httpBatchLink.js"
    },
    "./links/httpLink": {
      "import": "./dist/links/httpLink.mjs",
      "require": "./dist/links/httpLink.js",
      "default": "./dist/links/httpLink.js"
    },
    "./links/loggerLink": {
      "import": "./dist/links/loggerLink.mjs",
      "require": "./dist/links/loggerLink.js",
      "default": "./dist/links/loggerLink.js"
    },
    "./links/splitLink": {
      "import": "./dist/links/splitLink.mjs",
      "require": "./dist/links/splitLink.js",
      "default": "./dist/links/splitLink.js"
    },
    "./links/wsLink": {
      "import": "./dist/links/wsLink.mjs",
      "require": "./dist/links/wsLink.js",
      "default": "./dist/links/wsLink.js"
    },
    "./unstable-internals": {
      "import": "./dist/unstable-internals.mjs",
      "require": "./dist/unstable-internals.js",
      "default": "./dist/unstable-internals.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "!**/*.test.*",
    "README.md",
    "dist",
    "links",
    "package.json",
    "src",
    "unstable-internals"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "dev": "pnpm build --watch",
    "codegen-entrypoints": "tsx entrypoints.script.ts",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --ignore-path ../../.gitignore src",
    "ts-watch": "tsc --watch"
  },
  "peerDependencies": {
    "@trpc/server": "^10.45.1"
  },
  "dependencies": {},
  "devDependencies": {
    "@trpc/server": "^10.45.1",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.0",
    "rollup": "^4.9.5",
    "tslib": "^2.5.0",
    "tsx": "^4.0.0",
    "undici": "^6.0.1"
  },
  "eslintConfig": {
    "rules": {
      "no-restricted-imports": [
        "error",
        "@trpc/client"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
